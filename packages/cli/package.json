{
  "name": "untitled-ds",
  "version": "0.1.2",
  "description": "CLI for Untitled Design System - add components to your project",
  "type": "module",
  "bin": {
    "untitled-ds": "./dist/index.js"
  },
  "files": [
    "dist",
    "registry"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --clean",
    "dev": "tsup src/index.ts --format esm --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "execa": "^9.5.2",
    "ora": "^8.1.1",
    "picocolors": "^1.1.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/prompts": "^2.4.9",
    "tsup": "^8.3.5",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "design-system",
    "components",
    "cli",
    "react",
    "tailwind"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/untitled-design-system"
  }
}
